package com.skillstorm;

public class DataTypes {

	public static void main(String[] args) {
		
		/**
		 * Primitive
		 * - int (Integer 32 bit) **
		 * - long (Integer 64 bit) *
		 * - short (Integer 16 bit)
		 * - byte (Integer 8 bit)
		 * - float (Floating Point / Decimal 32 bit) *
		 * - double (Floating Point 64 bit) **
		 * - char (Character)
		 * - boolean (True/False) **
		 * 
		 * Non Primitive
		 * - String **
		 * - Array
		 * - Classes (Dog, Cat, String, etc.)
		 */
		
		int x = 10; // Declaring a variable
	}

}
